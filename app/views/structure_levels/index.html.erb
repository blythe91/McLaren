<p id="notice"><%= notice %></p>

<h1>Structure Levels</h1>

<table>
  <thead>
    <tr>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @structure_levels.each do |structure_level| %>
      <tr>
        <td><%= structure_level.description %></td>
        <td><%= link_to 'Show', structure_level %></td>
        <td><%= link_to 'Edit', edit_structure_level_path(structure_level) %></td>
        <td><%= link_to 'Destroy', structure_level, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Structure Level', new_structure_level_path %>
