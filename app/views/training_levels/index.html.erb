<p id="notice"><%= notice %></p>

<h1>Training Levels</h1>

<table>
  <thead>
    <tr>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @training_levels.each do |training_level| %>
      <tr>
        <td><%= training_level.description %></td>
        <td><%= link_to 'Show', training_level %></td>
        <td><%= link_to 'Edit', edit_training_level_path(training_level) %></td>
        <td><%= link_to 'Destroy', training_level, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Training Level', new_training_level_path %>
