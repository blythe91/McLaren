<p id="notice"><%= notice %></p>

<h1>Registries</h1>

<table>
  <thead>
    <tr>
      <th>Registry date</th>
      <th>Group</th>
      <th>Charge</th>
      <th>Unity</th>
      <th>Program receptor</th>
      <th>Person</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @registries.each do |registry| %>
      <tr>
        <td><%= registry.registry_date %></td>
        <td><%= registry.scout_group %></td>
        <td><%= registry.charge.to_s %></td>
        <td><%= registry.unity.to_s %></td>
        <td><%= registry.full_name_recep %></td>
        <td><%= registry.person %></td>
        <td><%= link_to 'Show', registry %></td>
        <td><%= link_to 'Edit', edit_registry_path(registry) %></td>
        <td><%= link_to 'Destroy', registry, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Registry', new_registry_path %>
